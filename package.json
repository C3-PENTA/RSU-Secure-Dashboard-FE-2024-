{
  "name": "rsu-dashboard-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "prepare": "test -d node_modules/husky && husky install || echo \"husky is not installed\"",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:stg": "tsc && vite build --mode staging",
    "preview": "vite preview",
    "lint": "tsc && eslint ./src --ext ts,tsx --quiet && prettier --check \"src/**/*.{ts,tsx,scss}\"",
    "lint--fix": "eslint ./src --ext ts,tsx --quiet --fix && prettier --write \"src/**/*.{ts,tsx,scss}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss}\""
  },
  "dependencies": {
    "@mantine/core": "^6.0.8",
    "@mantine/dates": "^6.0.14",
    "@mantine/form": "^6.0.13",
    "@mantine/hooks": "^6.0.8",
    "@mantine/notifications": "^6.0.8",
    "axios": "^0.27.2",
    "chart.js": "^4.3.0",
    "chartjs-plugin-annotation": "^3.0.1",
    "chartjs-plugin-datalabels": "^2.2.0",
    "chartjs-plugin-zoom": "^2.0.1",
    "dayjs": "^1.11.2",
    "file-saver": "^2.0.5",
    "i18next": "^21.8.5",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-i18next": "^11.16.9",
    "react-icons": "^4.9.0",
    "react-kakao-maps-sdk": "^1.1.9",
    "react-router-dom": "^6.3.0",
    "reactflow": "11.7.0",
    "rxjs": "^7.5.5",
    "socket.io-client": "^4.5.1",
    "tabler-icons-react": "^1.56.0",
    "universal-cookie": "^4.0.4",
    "vite-plugin-svgr": "^2.1.0",
    "vite-tsconfig-paths": "^3.5.0",
    "xlsx": "^0.18.5",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/file-saver": "^2.0.5",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^9.0.11",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "typescript": "^4.7.2",
    "vite": "^2.9.9"
  }
}
